{
   "extends": "expo/tsconfig.base",
   "compilerOptions": {
      "strict": true,
      "baseUrl": ".",
      "paths": {
         "@/components": ["src/components/index"],
         "@/screens/*": ["src/screens/*"],
         "@/services/*": ["src/services/*"],
         "@/types": ["src/types/index"],
         "@/constants/*": ["src/constants/*"],
         "@/hooks": ["src/hooks/index"],
         "@/utils/*": ["src/utils/*"]
      }
   }
}
